{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "indent": "off",
    "max-len": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "linebreak-style": ["error", "windows"]
  },
  "extends": ["eslint:recommended", "google"]
}